<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.googlemapsapiassignment.HelloController" stylesheets="@style.css">
  <padding>
    <Insets top="10" right="10" bottom="10" left="10" />
  </padding>
  <TextField fx:id="locationField" promptText="Enter Location" />
  <Button fx:id="fetchButton" text="Get Coordinates" />
  <TextArea fx:id="resultArea" prefHeight="200" prefWidth="300" editable="false" />
</VBox>
